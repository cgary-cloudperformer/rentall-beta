<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IsAvailable__c</fullName>
    <description>Formula field that determines if the product is available</description>
    <externalId>false</externalId>
    <formula>AvailabilityStart__c &lt;= NOW() &amp;&amp; (ISBLANK(AvailabilityEnd__c) || AvailabilityEnd__c &gt; NOW())</formula>
    <inlineHelpText>AvailabilityEnd__c</inlineHelpText>
    <label>Available</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
